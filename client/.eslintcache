n    //       variables: { accessArea: accessArea, tierId: tierId },\r\n    //     });\r\n    //     console.log(data);\r\n    //     return data;\r\n    //   } catch (err) {\r\n    //     console.error(err);\r\n    //   }\r\n    // };\r\n\r\n    // if data isn't here yet, say so\r\n    return (\r\n      <>\r\n        <Jumbotron fluid className=\"text-light bg-dark\">\r\n          <Container>\r\n            <h1>Viewing tiers</h1>\r\n          </Container>\r\n        </Jumbotron>\r\n        <Container>\r\n          <h2>\r\n            {tiersLength\r\n              ? `Viewing saved tiers`\r\n              : \"You have no tiers!\"}\r\n          </h2>\r\n          <CardColumns>\r\n            {this.props.tiers?.data?.tiers?.map((tier) => {\r\n              return (\r\n                <Card key={tier.tierName} border=\"dark\">\r\n                  <Card.Body>\r\n                    <Card.Title><h2>{tier.tierName}</h2></Card.Title>\r\n                    {/* {console.log(tier.permissions)} */}\r\n                    <Card.Text>\r\n                      {tier.permissions.map((permission) => (\r\n                        <ListGroup>\r\n                          {permission.accessArea}\r\n                        </ListGroup>\r\n                      ))}\r\n                    </Card.Text>\r\n                    {/*\r\n                  <Card.Text>\r\n                    {tier.users.map((user) => (\r\n                      <>\r\n                        <p key={user.event}></p>\r\n                        <p key={user.area}></p>\r\n                        <p key={user.creator}></p>\r\n                        <p kry={user.tier}></p>\r\n                      </>\r\n                    ))}\r\n                  </Card.Text> */}\r\n                    <Button\r\n                      className=\"btn-block btn-dark\"\r\n                      // onClick={() => handleAddToUser(event._id)}\r\n                    >\r\n                      Assign to User!\r\n                    </Button>\